{
  "version": 2,
  "builds": [
    {
      "src": "index.js", 
      "use": "@vercel/node"
    },
    {
      "src": "angular.json", // Adjust the path accordingly
      "use": "@vercel/static-build" // Use static-build for Angular projects
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}
