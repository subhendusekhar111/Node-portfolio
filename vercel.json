{
  "version": 2,
  "builds": [
    {
      "src": "index.js", 
      "use": "@vercel/node"
    },
    {
      "src": "package.json", 
      "use": "@vercel/node"
    },
    {
      "src": ".angular.json",
      "use": "@angular/cli"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.html"
    }
  ]
}
